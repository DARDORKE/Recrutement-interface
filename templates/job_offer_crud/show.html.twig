{% extends 'base.html.twig' %}

{% block title %}JobOffer{% endblock %}

{% block body %}
    <h1>JobOffer</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ job_offer.id }}</td>
            </tr>
            <tr>
                <th>JobTitle</th>
                <td>{{ job_offer.jobTitle }}</td>
            </tr>
            <tr>
                <th>Workplace</th>
                <td>{{ job_offer.workplace }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ job_offer.description }}</td>
            </tr>
            <tr>
                <th>IsPublished</th>
                <td>{{ job_offer.isPublished ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_job_offer_crud_index') }}">back to list</a>

    <a href="{{ path('app_job_offer_crud_edit', {'id': job_offer.id}) }}">edit</a>

    {{ include('job_offer_crud/_delete_form.html.twig') }}
{% endblock %}
